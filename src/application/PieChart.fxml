<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PieChartController">
   <children>
      <Label layoutX="35.0" layoutY="50.0" prefHeight="17.0" prefWidth="225.0" text="Enter number of divisions ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="fieldInput" layoutX="283.0" layoutY="44.0" prefHeight="25.0" prefWidth="108.0">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button fx:id="submit" layoutX="422.0" layoutY="49.0" mnemonicParsing="false" onAction="#submitAction" text="submit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TableView layoutX="51.0" layoutY="115.0" prefHeight="387.0" prefWidth="409.0">
        <columns>
          <TableColumn fx:id="DivNum" prefWidth="110.0" text="DivNum" />
          <TableColumn minWidth="0.0" prefWidth="162.0" text="DivName" />
            <TableColumn prefWidth="136.0" text="percentage" />
        </columns>
      </TableView>
      <Button fx:id="saveInput" layoutX="224.0" layoutY="531.0" mnemonicParsing="false" onAction="#showWindow" text="save" textFill="#21c92c">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
